# переприсваивание значений в переменные

variable_1 = "value 1"
variable_2 = "value 2"

print("Переменная variable_1 имеет значение: ", variable_1)
print("Переменная variable_2 имеет значение: ", variable_2)

# переприсвоим значение variable_2 переменной variable_1
variable_1 = variable_2

print('Теперь переменная variable_1 имеет значение: ', variable_1)
